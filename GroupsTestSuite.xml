<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Group_Test_Suites" verbose="1">

    <groups>
        <run>
            <include name="smoke"/>
            <exclude name="reg"/>
        </run>
    </groups>


    <test name="TestNG_Groups">
        <classes>
            <class name="org.testng.groups.test.GroupsTest1"/>
            <class name="org.testng.groups.test.GroupsTest2"/>
            <class name="org.testng.groups.test.GroupsTest3"/>
            <class name="org.testng.groups.test.GroupsTest4"/>
        </classes>
    </test>
    
</suite>